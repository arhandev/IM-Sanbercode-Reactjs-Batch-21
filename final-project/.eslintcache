30","131"],"import React, {useEffect, useContext, useState} from 'react'\nimport axios from 'axios'\nimport {DataListContext} from '../../context/DataListContext'\nimport {\n    Link\n  } from 'react-router-dom'\nimport { Card } from 'antd';\nimport { Typography } from 'antd';\n\nconst { Meta } = Card;\nconst { Title } = Typography;\n\nfunction Games() {\n    const {game} = useContext(DataListContext)\n    const [gameData, setGameData] = game\n\n    const fetchFilm = () =>{\n        axios.get(\"https://backendexample.sanbersy.com/api/data-game\")\n        .then(response =>{\n            setGameData(response.data)\n\n        })\n        .catch(error =>\n            console.log(error.messages))\n    }\n\n    useEffect(()=>{\n        if(gameData === null){\n            fetchFilm()\n        }\n    },[gameData])\n\n    const getList = (data) => {\n        if(data){\n        var out=[];\n        var count = 0;\n        for (let i = 0; i < data.length; i += 5) {\n          out[count] = data.slice(i, i + 5);\n          count++;\n        }\n        return out;}\n        return null\n      };\n\n    const data = getList(gameData);\n\n\n    return (\n        <div style={{margin: \"20px\"}}>\n            <Title>LIST MOVIE</Title>\n            {\n                data ? \n                data.map(row => (\n                    <div style={{display:\"flex\", justifyContent: \"flex-start\", margin: \"20px 0\"}}>\n                        {\n                            row.map(item=>(\n                                <div key={item.id} style={{margin: \"0 1rem\"}}>\n                                    <Link to={`/games/${item.id}`}>\n                                        <Card\n                                            hoverable\n                                            style={{ width: 240 }}\n                                            cover={<img alt=\"example\" src={item.image_url} style={{height:\"20rem\"}} />}\n                                        >\n                                            <Meta title={item.name} description={`Genre: ${item.genre}`} />\n                                        </Card>\n                                    </Link>\n                                </div>\n                                )\n                            )\n                        }\n\n                    </div>\n\n                )):\n\n                <h1>LOADING</h1>\n            }\n        </div>\n    )\n}\n\nexport default Games\n","/home/supiraru/Data/coding/IM-Sanbercode-Reactjs-Batch-21/final-project/src/routes/Movies/Movies.js",["132","133"],"import React, {useEffect, useContext, useState} from 'react'\nimport axios from 'axios'\nimport {DataListContext} from '../../context/DataListContext'\nimport {\n    Link\n  } from 'react-router-dom'\n  import { Card } from 'antd';\n  import { Typography } from 'antd';\n\n\nconst { Meta } = Card;\nconst { Title } = Typography;\n\n\nfunction Movies() {\n    const {film} = useContext(DataListContext)\n    const [filmData, setFilmData] = film\n\n    const fetchFilm = () =>{\n        axios.get(\"https://backendexample.sanbersy.com/api/data-movie\")\n        .then(response =>{\n            setFilmData(response.data)\n\n        })\n        .catch(error =>\n            console.log(error.messages))\n    }\n\n    useEffect(()=>{\n        if(filmData === null){\n            fetchFilm()\n        }\n    },[filmData])\n\n    const getList = (data) => {\n        if(data){\n        var out=[];\n        var count = 0;\n        for (let i = 0; i < data.length; i += 5) {\n          out[count] = data.slice(i, i + 5);\n          count++;\n        }\n        return out;}\n        return null\n      };\n\n    const data = getList(filmData);\n\n    return (\n        <div style={{margin: \"20px\"}}>\n            <Title>LIST MOVIE</Title>\n            {\n                data ? \n                data.map(row => (\n                    <div style={{display:\"flex\", justifyContent: \"flex-start\", margin: \"20px 0\