n            <div className=\"search-books\">\n                <input onChange={event => {setSearch(event.target.value);}} type=\"text\"/><button onClick={searchData}>Search</button>\n            </div>\n\n            <div className=\"table-books\">\n                <table>\n                <thead>\n                    <tr>\n                        <th>\n                            No\n                        </th>\n                        <th>\n                            Title\n                        </th>\n                        <th>\n                            Description\n                        </th>\n                        <th>\n                            Review\n                        </th>\n                        <th>\n                            Release Year\n                        </th>\n                        <th>\n                            Total Page\n                        </th>\n                        <th>\n                            Price\n                        </th>\n                        <th>\n                            Action\n                        </th>\n                    </tr>\n                </thead>\n                {\n                    daftarBuku ?\n                    hasilDataBuku?\n                    hasilDataBuku.map((item, index) =>(\n                        <tbody >\n                            <tr>\n                                <td>\n                                    {index+1}\n                                </td>\n                                <td>\n                                    {item.title}\n                                </td>    \n                                <td>\n                                  {item.description}  \n                                </td>\n                                <td>\n                                    {item.review}\n                                </td>\n                                <td>\n                                    {item.release_year}\n                                </td>\n                                <td>\n                                    {item.totalPage}\n                                </td>\n                                <td>\n                                    {item.price}\n                                </td>\n                                <td>\n                                 <button onClick={() => editData(index, item.id)}>\n                                        edit\n                                    </button>\n                                    <button onClick={() =>deleteData(item.id)}>\n                                        delete\n                                    </button>\n                                </td>\n                            </tr>      \n                        </tbody>\n                    )):\n                    daftarBuku.map((item, index) =>(\n                        <React.Fragment key={index}>\n                        <tbody >\n                            <tr>\n                                <td>\n                                    {index+1}\n                                </td>\n                                <td>\n                                    {item.title}\n                                </td>    \n                                <td>\n                                  {item.description}  \n                                </td>\n                                <td>\n                                    {item.review}\n                                </td>\n                                <td>\n                                    {item.release_year}\n                                </td>\n                                <td>\n                                    {item.totalPage}\n                                </td>\n                                <td>\n                                    {item.price}\n                                </td>\n                                <td>\n                                    <button onClick={() => editData(index, item.id)}>\n                                        edit\n                                    </button>\n                                    <button onClick={() =>deleteData(item.id)}>\n                                        delete\n                                    </button>\n                                </td>\n                            </tr>      \n                        </tbody>\n                        </React.Fragment>\n                    )):\n                    <tbody>\n                        <tr>\n                            <td>\n                            LOADING\n                            </td>\n                        </tr>\n                    </tbody>\n                }\n\n                </table>\n            </div>\n            <br/>\n            <div className=\"form-books\">\n                <form onSubmit={submitData}>\n                    <h3>\n                        Books Form\n                    </h3>\n                    <div className=\"div-input-form\">\n                        <label>\n                            Title:\n                        </label>\n                        <input className=\"input-form\" type=\"text\" value={inputTitle} onChange={event => {setInputTitle(event.target.value);}}/>\n                    </div>\n                    <div className=\"div-input-form\">\n                        <label>\n                            Description:\n                        </label>\n                        <textarea className=\"input-form\" value={inputDescription} onChange={event => {setInputDescription(event.target.value);}}/>\n                    </div>\n                    <div className=\"div-input-form\">\n                        <label>\n                            Review:\n                        </label>\n                        <textarea className=\"input-form\" value={inputReview}  onChange={event => {setInputReview(event.target.value);}}/>\n                    </div>\n                    <div className=\"div-input-form\">\n                        <label>\n                            Release Year: \n                        </label>\n                        <input className=\"input-form\" value={inputReleaseYear} type=\"number\" onChange={event => {setInputReleaseYear(event.target.value);}}/>\n                    </div>\n                    <div className=\"div-input-form\">\n                        <label>\n                            Total Page : \n                        </label>\n                        <input className=\"input-form\" type=\"number\" value={inputTotalPage} onChange={event => {setInputTotalPage(event.target.value);}}/>\n                    </div>\n                    <div className=\"div-input-form\">\n                        <label>\n                            Price: \n                        </label>\n                        <input className=\"input-form\" value={inputPrice} type=\"number\" onChange={event => {setInputPrice(event.target.value);}} />\n                    </div>\n                    <div className=\"div-input-form\">\n                        <label>\n                            Image Url\n                        </label>\n                        <textarea className=\"input-form\" value={inputImageUrl} type=\"text\" onChange={event => {setInputImageUrl(event.target.value);}}/>\n                    </div>\n                    <button type=\"submit\">\n                        click me\n                    </button>\n                </form>\n            </div>\n        </div>\n    )\n}\n\nexport default Books\n",["52","53"],{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":6,"column":8,"nodeType":"60","messageId":"61","endLine":6,"endColumn":13},{"ruleId":"58","severity":1,"message":"62","line":12,"column":3,"nodeType":"60","messageId":"61","endLine":12,"endColumn":7},{"ruleId":"63","severity":1,"message":"64","line":27,"column":13,"nodeType":"65","endLin